{
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["../frontend", "../backend"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}